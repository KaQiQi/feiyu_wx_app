/*弹出动画  */

.list_item {
  animation: mytranstion 2s linear;
  -moz-animation: mytranstion 2s linear;
  -webkit-animation: mytranstion 2s linear;
  -o-animation: mytranstion 2s linear;
}

@keyframes mytranstion {
  0% {
    margin-left: 0;
  }

  5% {
    margin-left: 33px;
  }

  10% {
    margin-left: 66px;
  }

  15% {
    margin-left: 99px;
  }

  20% {
    margin-left: 133px;
  }

  25% {
    margin-left: 166px;
  }

  30% {
    margin-left: 200px;
  }

  50% {
    margin-left: 200px;
  }

  100% {
    margin-left: 200px;
  }
}

