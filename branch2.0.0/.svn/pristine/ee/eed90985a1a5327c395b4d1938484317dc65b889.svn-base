/**app.wxss**/
/* 间隔区域 */
#none{
  width: 100%;
  height: 100rpx;
}
view,text,image,scroll-view,cover-view,input{
  box-sizing: border-box;
}
.live_gap {
  display: block;
  height: 12rpx;
  background-color: #f2f2f2;
}

.rightArrow {
  width: 17rpx;
  height: 24rpx;
}

.nodata_icon {
  /*图片居中  */
  display: block;
  margin: auto;
  margin-top: 100rpx;
  width: 226rpx;
  height: 208rpx;
}

/*所有的下一步按钮通用样式  */
.next_btn {
  margin-top: 68rpx;
  margin-left: 28rpx;
  margin-right: 28rpx;
  background-color: #e72e4b;
  height: 80rpx;
  border-radius:12rpx;
  text-align: center;
  color: #ffffff;
  font-size:32rpx;
  font-family: FZLTXHK;
  line-height: 80rpx;
}
.main_color{
  color: #e72f4b;
}

.clearfix::after {
  display: block;
  content: "";
  clear: both;
}

.fl {
  float: left;
}
.no_border{
  border: none;
}
.fr {
  float: right;
}
.loading_image{
  margin-top: 400rpx;
  margin-left: 275rpx;
  width: 200rpx;
  height: 90rpx;
  display: block;
}
button.form_button{background-color:transparent;padding:0;margin:0;display:inline;position:static;border:0;padding-left:0;padding-right:0;border-radius:0;font-size:0rpx;color:transparent;}

button.form_button::after{content:'';width:0;height:0;-webkit-transform:scale(1);transform:scale(1);display:none;background-color:transparent;}
.share {
  z-index: 99999999;
  position:fixed;
  top: 40rpx;
  right: 20rpx;
}
.m_share {
  z-index: 99999999;
  position:fixed;
  top: 100rpx;
  right: 20rpx;
}
.go_index{
  width:70rpx;
  height:70rpx;
  display: block;
}
.no_margin{
  margin: 0;
}