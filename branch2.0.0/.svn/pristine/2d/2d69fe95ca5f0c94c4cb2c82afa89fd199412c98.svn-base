  font-weight: 500;
}

.pro_price {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  font-weight: 800;
  margin-top: 16rpx;
  margin-bottom: 18rpx;
}


.other_products {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background-color: #f2f2f2;
  padding: 10rpx 20rpx;
  justify-content: space-between;
}

.product_container {

  width: 345rpx;
  margin-top: 20rpx;
  padding: 0 0 20rpx;
  background-color: #fff;
}
.fixed{
  position: relative;
}

.hot_img {
  width: 345rpx;
  height: 345rpx;
  display: block;
}

.other_brand_name {
  /* height: 34rpx; */
  display: flex;
  align-items: center;
  color: #333;
  font-weight: 700;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.other_brand_name>text {
  margin-right: 6rpx;
}

