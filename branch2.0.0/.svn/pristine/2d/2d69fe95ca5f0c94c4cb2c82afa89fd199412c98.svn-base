  color: #e72f4b;
  font-weight: 700;
  font-size: 24rpx;
  margin-top: 16rpx;
}



/* sku */
/* sku弹窗 */
.product_sku {
  width: 100%;
  height: 800rpx;
  overflow: scroll;
  background-color: #fff;
  position: fixed;
  left: 0;
  z-index: 99999999;
  padding: 0 30rpx;
  bottom: -800rpx;
  transition: all 0.5s;
}

#bottom, #shopbtm, #coupon_mask,#choose_coupon {
  bottom: 0;
}

.brand {
  display: flex;
  width: 100%;
  padding: 30rpx 0;
}

.brand_img image {
  display: block;
  width: 220rpx;
  height: 220rpx;
}

.brand_desc text {
  display: block;
}

.sku_brand_desc {
  margin-left: 26rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 24rpx;
  color: #333;
}

.sku_brand_price {
  font-size: 26rpx;
  color: #e72f4b;
}

.sku_brand_name {
  color: #333;
  font-size: 30rpx;
  margin-top: 16rpx;
}

.spec {
  width: 100%;
  padding: 24rpx 0;
  border-top: 1px solid #f2f2f2;
}
.spec .s_title {
  display: block;
  font-size: 28rpx;
  color: #333;
}

.spec_desc {
  display: flex;
  padding: 0 0 24rpx;
  border-bottom: 1px solid #f2f2f2;
  flex-wrap: wrap;
}

.spec_desc text {
  display: block;
  color: #333;
  font-size: 24rpx;
  background-color: #f2f2f2;
  height: 54rpx;
  border-radius: 27rpx;
  line-height: 54rpx;
  padding: 0 28rpx;
  margin-right: 26rpx;
  margin-top: 24rpx;
  overflow: hidden;
}
.shop_num{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:20rpx 0 0;
}
.shop_num .fl_container {
  font-size: 30rpx;
  color: #333;
}

.shop_num .fr_container {
  display: flex;
  border: 1px solid #eeeeee;
  border-radius: 8rpx;
}

.shop_num .fr_container text, .shop_num .fr_container input {
  display: block;
  width: 68rpx;
  height: 60rpx;
  margin-right: 2rpx;
  line-height: 60rpx;
  color: #333;
  font-size: 26rpx;
  text-align: center;
}

.shop_num .fr_container text.num_pro {
  color: #333;
  border-left: none;
  border-right: none;
}

.shop_num text.number {
  color: #666;
}

.shop_num .fr_container .onlyOne {
  color: #ccc;
}

.spec_desc text.active {
  background-color: #e72f4b;
  color: #fff;
}

/* 售罄 */
#sale_none{
  color: #ffffff;
  background-color: #aaaaaa;
}

.confirm {
  display: block;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: #e72f4b;
  text-align: center;
  position: fixed;
  bottom: 0;
  left: 0;
}

.close {
  position: absolute;
  top: 22rpx;
  right: 30rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.close image {
  width: 26rpx;
  height: 26rpx;
}

#mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}

view.noStock{
  background-color: #aaaaaa;
  color: #ffffff;
}
#noStock{
  background-color: #f2f2f2;
  color: #bbbbbb;
}

page {
  width: 100%;
  height: 100%;
}


.noscroll {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.confirm_placeholder{
  width: 100%;
  height: 90rpx;
}

.time_container {
  width: 98rpx;
  height: 41rpx;
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.time_icon {
  display: block;
  width: 98rpx;
  height: 41rpx;
}

.time_num {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  top: 50%;
  right: 12rpx;
  transform: translateY(-50%);
}

