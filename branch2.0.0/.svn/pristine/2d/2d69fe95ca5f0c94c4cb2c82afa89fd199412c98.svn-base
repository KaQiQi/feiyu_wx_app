}
.words_desc{
    width: 710rpx;
    padding:32rpx 40rpx;
    position: absolute;
    top: 360rpx;
    left: 50%;
    transform: translateX(-50%);
    background-color: #ffffff;
    font-size: 24rpx;
    color: #333333;
    line-height: 40rpx;
    border-bottom: 2rpx solid #f2f2f2;
}
.palceholder{
    width: 100%;
    height: 102rpx;
    background-color: #f2f2f2;
}
.new_products {
    display: flex;
    flex-wrap: wrap;
    padding: 0rpx 20rpx;
    justify-content: space-between;
    background-color: #f2f2f2;
}
.brand_desc{
    /* text-align: center; */
    font-size: 22rpx;
    color: #333333;
    /* margin-top: 14rpx; */
    /* padding: 0 20rpx; */
    overflow: hidden;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    text-overflow: ellipsis;
}
 .products_detail .pro_img {
    width: 340rpx;
    height: 340rpx;
    display: block;
}
.products_detail {
    background-color: #ffffff;
    position: relative;
