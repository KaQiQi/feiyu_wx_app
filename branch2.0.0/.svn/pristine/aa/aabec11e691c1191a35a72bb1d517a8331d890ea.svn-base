
page{
    width: 100%;
    height: 100%;
    background-color: #f2f2f2;
}
.top_tips{
    width: 100%;
    height: 130rpx;
}
.tips_img{
    width: 100%;
    height: 130rpx;
    display: block;
}
.out_container{
    background-color: #ffffff;
    margin-top: 20rpx;
    width: 100%;
    padding: 0 20rpx;
}
.top_content{
    width: 100%;
    border-bottom: 1rpx solid #dddddd;
    padding: 20rpx 0;
}
.brand_name{
    color: #333333;
    font-size: 28rpx;
}
.time_no{
    color: #333333;
    margin-top: 16rpx;
    font-size: 28rpx;
}
.btn_container{
    width: 100%;
    padding: 0 44rpx;
    margin: 50rpx 0;
    display: flex;
    justify-content: space-between;
}
.btn_container>view{
    display: flex;
    width: 270rpx;
    height: 80rpx;
    border: 1rpx solid #333333;
    border-radius: 40rpx;
    font-size: 26rpx;
    color: #333333;
    justify-content: center;
    align-items: center;
}