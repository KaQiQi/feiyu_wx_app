/* tab栏 */
@import "../../page/cmsSingle/cmsSingle.wxss";
@import "/pages/livePlayer/reviewlive/reviewlive.wxss";

.tabbar {
  width: 100%;
  display: flex;
  background-color: #fff;
  border-bottom: 1rpx solid #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999999999;
  height: 88rpx;
  border-top: 1rpx solid #f2f2f2;
}

.swiper {
  width: 750rpx;
  height: 280rpx;
}

swiper-item {
  display: block;
}

.tabbar>view {
  flex: 1;
}

.banner_image {
  display: block;
  width: 100%;
  height: 280rpx;
}

.placeholder {
  width: 100%;
  height: 88rpx;
}

.live_detail {
  width: 100%;
  padding: 0 20rpx;
}

.live_words_desc {
  width: 100%;
  overflow: hidden;
  margin-left: 20rpx;
  height: 120rpx;
  position: relative;
}

.live_img>image {
  width: 120rpx;
  height: 120rpx;
  display: block;
}

.live_icon {
  width: 100rpx;
  height: 36rpx;
  margin-right: 8rpx;
}

.top_title {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #333;
  width: 100%;
}

.right_icon {
  width: 12rpx;
  height: 22rpx;
  margin-left: 8rpx;
}

.live_status {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666;
  /* margin-top: 50rpx; */
  position: absolute;
  bottom: 20rpx;
  left: 0;
}

.alert {
  width: 24rpx;
  height: 24rpx;
  margin-right: 8rpx;
  vertical-align: middle;
}

/* #word_care{
      margin-top: 1px;
  } */

.flex_left, .flex_right {
  font-size: 22rpx;
  display: flex;
  align-items: center;
}

.fixed {
  position: relative;
  margin-bottom: 20rpx;
}

.live_trailer {
  width: 61rpx;
  height: 36rpx;
  margin-right: 8rpx;
}

.content {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  color: #1e1e1e;
  justify-content: center;
  position: relative;
}

.content .num {
  font-size: 32rpx;
  font-weight: 900;
  margin-bottom: 6rpx;
}

.home_icon {
  width: 37rpx;
  height: 34rpx;
  display: block;
  margin-bottom: 10rpx;
  margin-top: 6rpx;
}

.content .border {
  width: 90rpx;
  height: 4rpx;
  position: absolute;
  left: 50%;
  bottom: -1rpx;
  transform: translateX(-50%);
}

.active {
  background-color: #e72f4b;
}

#active {
  color: #e72f4b;
}

.live_block {
  width: 100%;
  position: relative;
}

.live_bg_img {
  width: 750rpx;
  height: 320rpx;
  display: block;
}

.live_shop_desc {
  width: 100%;
  position: absolute;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  flex-direction: column;
  bottom: 28rpx;
}

.sale_title {
  margin-bottom: 10rpx;
  color: #fff;
  font-size: 28rpx;
}

.content_desc {
  color: #fff;
  font-size: 28rpx;
}

.products {
  width: 100%;
  overflow: scroll;
  white-space: nowrap;
  display: flex;
  background-color: #fff;
  box-sizing: border-box;
  padding: 20rpx;
}

.pro_container {
  position: relative;
  display: inline-block;
  margin-right: 20rpx;
  width: 200rpx;
  height: 200rpx;
  color: #333;
  font-size: 24rpx;
}

.products_img {
  display: block;
  width: 200rpx;
  height: 200rpx;
}

.p_brand_name {
  width: 100%;
  font-size: 24rpx;
  overflow: hidden;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  white-space: pre-line;
  text-align: center;
  margin: 16rpx 0 10rpx;
}

.p_pro_price {
  display: flex;
  width: 100%;
  justify-content: center;
  margin-bottom: 26rpx;
}

.block_title {
  position: relative;
  font-size: 30rpx;
  color: #323333;
  padding: 30rpx 0 22rpx 25rpx;
  background-color: #f2f2f2;
  display: flex;
}

.block_icon {
  display: block;
  margin: auto 0;
  margin-right: 12rpx;
  width: 5rpx;
  height: 30rpx;
}

.new_products {
  display: flex;
  flex-wrap: wrap;
  padding: 0rpx 20rpx;
  justify-content: space-between;
  background-color: #f2f2f2;
}

.brand_desc {
  text-align: center;
  font-size: 22rpx;
  color: #333;
  margin-top: 14rpx;
  padding: 0 20rpx;
  overflow: hidden;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
}

.products_detail .pro_img {
  width: 345rpx;
  height: 345rpx;
  display: block;
}

.products_detail {
  background-color: #fff;
  position: relative;
  width: 345rpx;
  margin-bottom: 18rpx;
  border-bottom: 1rpx solid #f2f2f2;
}

.pro_name {
  text-align: center;
  overflow: hidden;
  width: 100%;
  font-size: 24rpx;
  color: #333;
  margin-top: 18rpx;
  font-weight: 500;
}

.pro_price {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  font-weight: 800;
  margin-top: 16rpx;
  margin-bottom: 18rpx;
}

.button {
  font-size: 22rpx;
  color: #666;
  background-color: transparent;
  padding: 0;
  border: 0;
  margin: 0;
  display: flex;
  align-items: center;
  outline: none;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 6rpx;
  padding-left: 0;
  padding-right: 0;
  border-radius: 0;
}

button::after {
  border: none;
}

.lives_container {
  width: 100%;
  border-bottom: 1px solid #f2f2f2;
  position: relative;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.other_products {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background-color: #f2f2f2;
  padding: 10rpx 20rpx;
  justify-content: space-between;
}

.time_container {
  width: 98rpx;
  height: 41rpx;
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.time_icon {
  display: block;
  width: 98rpx;
  height: 41rpx;
}

.time_num {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  top: 50%;
  right: 12rpx;
  transform: translateY(-50%);
}

.product_container {
  width: 345rpx;
  margin-top: 20rpx;
  padding: 0 0 20rpx;
  background-color: #fff;
}

.hot_img {
  width: 345rpx;
  height: 345rpx;
  display: block;
}

.other_brand_name {
  /* height: 34rpx; */
  display: flex;
  align-items: center;
  color: #333;
  font-weight: 700;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.other_brand_name>text {
  margin-right: 6rpx;
}

.pro_describle {
  padding: 0 20rpx;
  color: #333;
  font-size: 22rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopcar_icon {
  width: 30rpx;
  height: 26rpx;
}

.other_hot_price {
  padding: 0 26rpx 0 20rpx;
  display: flex;
  justify-content: space-between;
  color: #e72f4b;
  font-weight: 700;
  font-size: 24rpx;
  margin-top: 16rpx;
}

.loading_image {
  margin-top: 400rpx;
  margin-left: 275rpx;
  width: 200rpx;
  height: 90rpx;
  display: block;
}

/* sku */

/* sku弹窗 */

.product_sku {
  width: 100%;
  height: 800rpx;
  overflow: scroll;
  background-color: #fff;
  position: fixed;
  left: 0;
  z-index: 99999999;
  padding: 0 30rpx;
  bottom: -800rpx;
  transition: all 0.5s;
}

.confirm_placeholder {
  width: 100%;
  height: 90rpx;
}

.s_c_container {
  border-bottom: 16rpx solid #f2f2f2;
}

#bottom, #shopbtm, #coupon_mask, #choose_coupon {
  bottom: 0;
}

.brand {
  display: flex;
  width: 100%;
  padding: 30rpx 0;
}

.brand_img image {
  display: block;
  width: 220rpx;
  height: 220rpx;
}

.brand_desc text {
  display: block;
}

.sku_brand_desc {
  margin-left: 26rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 24rpx;
  color: #333;
}

.sku_brand_price {
  font-size: 26rpx;
  color: #e72f4b;
}

.sku_brand_name {
  color: #333;
  font-size: 30rpx;
  margin-top: 16rpx;
}

.spec {
  width: 100%;
  padding: 24rpx 0;
  border-top: 1px solid #f2f2f2;
}

.spec .s_title {
  display: block;
  font-size: 28rpx;
  color: #333;
}

.spec_desc {
  display: flex;
  padding: 0 0 24rpx;
  border-bottom: 1px solid #f2f2f2;
  flex-wrap: wrap;
}

.spec_desc text {
  display: block;
  color: #333;
  font-size: 24rpx;
  background-color: #f2f2f2;
  height: 54rpx;
  border-radius: 27rpx;
  line-height: 54rpx;
  padding: 0 28rpx;
  margin-right: 26rpx;
  margin-top: 24rpx;
  overflow: hidden;
}

.shop_num {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0 0;
}

.shop_num .fl_container {
  font-size: 30rpx;
  color: #333;
}

.shop_num .fr_container {
  display: flex;
  border: 1px solid #eee;
  border-radius: 8rpx;
}

.shop_num .fr_container text, .shop_num .fr_container input {
  display: block;
  width: 68rpx;
  height: 60rpx;
  margin-right: 2rpx;
  line-height: 60rpx;
  color: #333;
  font-size: 26rpx;
  text-align: center;
}

.shop_num .fr_container text.num_pro {
  color: #333;
  border-left: none;
  border-right: none;
}

.shop_num text.number {
  color: #666;
}

.shop_num .fr_container .onlyOne {
  color: #ccc;
}

.spec_desc text.active {
  background-color: #e72f4b;
  color: #fff;
}

/* 售罄 */

#sale_none {
  color: #fff;
  background-color: #aaa;
}

.confirm {
  display: block;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: #e72f4b;
  text-align: center;
  position: fixed;
  bottom: 0;
  left: 0;
}

.close {
  position: absolute;
  top: 22rpx;
  right: 30rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.close image {
  width: 26rpx;
  height: 26rpx;
}

#mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}

view.noStock {
  background-color: #aaa;
  color: #fff;
}

#noStock {
  background-color: #f2f2f2;
  color: #bbb;
}

page {
  width: 100%;
  height: 100%;
}

.noscroll {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.time_container {
  width: 98rpx;
  height: 41rpx;
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.time_icon {
  display: block;
  width: 98rpx;
  height: 41rpx;
}

.time_num {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  top: 50%;
  right: 12rpx;
  transform: translateY(-50%);
}
