/* pages/activity/vipClub/applyResult/applyResult.wxss */
page{
    width: 100%;
    height: 100%;
}
.container{
    padding-top: 80rpx;
    display: flex;
    align-items: center;
    flex-direction: column;
    font-size: 26rpx;
    color: #333333;
}
.image{
    width: 517rpx;
    height: 200rpx;
}
.big_font{
    font-size: 30rpx;
    margin-top: 36rpx;
}
.small_icon{
    width: 20rpx;
    height: 12rpx;
    margin-top: 36rpx;
}
.top_banner{
    width: 750rpx;
    height: 562rpx;
    position: relative;
}
.contact_container{
    width: 96rpx;
    height: 96rpx;
    position: absolute;
    bottom: 20rpx;
    right: 20rpx;
    border-radius: 50%;
}
.contact_icon{
    width: 96rpx;
    height: 96rpx;
    display: block;
    border-radius: 50%;
}
.contact_btn{
    opacity: 0;
    position: absolute;
    top: 0;
    left: 0;
    width: 96rpx;
    height: 96rpx;
}
.banner_img{
    width: 750rpx;
    height: 562rpx;
    display: block;
}
.bm_container{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.top_tips{
    margin: 40rpx 0;
    color: #333333;
    font-size: 24rpx;
}
.card_num,.mobile_num,.verification_code,.send_btn{
    width: 690rpx;
    height: 80rpx;
    padding-left: 38rpx;
    font-size: 24rpx;
    color: #333333;
    background-color: #ffffff;
    border-radius: 10rpx;
    line-height: 80rpx;
}
.mobile_num{
    margin-top: 20rpx;
}
.verification{
    margin-top: 20rpx;
    height: 80rpx;
    width: 690rpx;
    display: flex;
}
.verification_code{
    width: 380rpx;
    margin-right: 30rpx;
}
.send_btn{
    width: 290rpx;
    text-align: center;
    padding-left: 0;
    background-color: #c89f62;
    color: #ffffff;
}
.info_btn{
    width: 100%;
    height: 100%;
    opacity: 0;
    position: absolute;
    top: 0;
    left: 0;
}
.activation_btn{
    width: 690rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    color: #ffffff;
    background-color: #c89f62;
    font-size: 28rpx;
}
.btn_container{
    margin-top: 40rpx;
    width: 690rpx;
    height: 80rpx;
    position: relative;
}
#sendding{
    color: #888888;
    background-color: #ffffff;
}
#bg_color{
    width: 100%;
    height: 100%;
    background-color: #d9d1c0;
}
.go_index{
    width: 70rpx;
    height: 70rpx;
    position: fixed;
    top: 30rpx;
    right: 30rpx;
  }

