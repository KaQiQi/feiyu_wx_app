/* pages/my/myCashBack/commitSuccess/commitSuccess.wxss */
page{
    width: 100%;
    height: 100%;
    background-color: #f2f2f2;
}
.top_tips{
    width: 100%;
    
    display: flex;
    flex-direction: column;
    align-items: center;
}
.top_icon{
    width: 120rpx;
    height: 115rpx;
    margin-top: 80rpx;
}
view{
    color: #333333;
    font-size: 26rpx;
}
.title{
    margin: 40rpx 0;
}
.bottom_desc{
    background-color: #ffffff;
    border-top: 1rpx solid #f2f2f2;
    width: 100%;
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
}
.success_icon{
    width: 650rpx;
    margin: 80rpx auto 0; 
    height: 88rpx;
    line-height: 88rpx;
    background-color: #e72f4b;
    color: #fffefe;
    border-radius: 6rpx;
    text-align: center;
    font-size: 28rpx;
    font-weight:0;
}