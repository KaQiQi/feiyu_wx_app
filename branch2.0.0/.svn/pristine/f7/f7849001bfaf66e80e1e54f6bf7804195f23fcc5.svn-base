page{
    width: 100%;
    height: 100%;
}
.swiper {
    width: 750rpx;
    height: 500rpx;
    border-top: 1rpx solid #f2f2f2;
    border-bottom: 1rpx solid #f2f2f2;
}

.slide_img {
    width: 750rpx;
    height: 500rpx;
}

.count {
    display: flex;
    justify-content: space-between;
    color: #888888;
    font-size: 24rpx;
    margin-bottom: 10rpx;
    margin-top: 10rpx;
}

.count_detail {
    width: 100%;
    padding: 20rpx;
}

progress {
    border-radius: 8rpx;
    overflow: hidden;
    margin-bottom: 30rpx;
}

.my_conut {
    font-size: 26rpx;
    color: #333333;
    display: flex;
    justify-content: space-between;
    padding-top: 20rpx;
    border-top: 1rpx solid #f2f2f2;
}

.brand_name {
    font-size: 28rpx;
    color: #333333;
}

.user_detail {
    width: 100%;
    border-top: 12rpx solid #f2f2f2;
    padding: 20rpx;
    background-color: #fff1f4;
    display: flex;
    position: relative;
}

.avatar {
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}

.avatar_img {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    display: block;
}

.right_desc {
    width: 100%;
    overflow: hidden;
}

.right_desc>view {
    margin-bottom: 10rpx;
    color: #333333;
    font-size: 24rpx;
}

.win_icon {
    width: 102rpx;
    height: 102rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}

.top_title {
    position: relative;
    display: flex;
    align-items: center;
    width: 100%;
    padding: 20rpx;
    font-size: 28rpx;
    color: #666666;
    border-top: 12rpx solid #f2f2f2;
}

.left_border {
    width: 4rpx;
    height: 28rpx;
    background-color: #e72f4b;
    margin-right: 10rpx;
}

.draw_btn {
    background-color: #ffffff;
    width: 100%;
    padding: 20rpx;
    position: fixed;
    bottom: 0;
    left: 0;
}

.draw_btn>view {
    width: 100%;
    border-radius: 8rpx;
    background-color: #e72f4b;
    color: #ffffff;
    font-size: 26rpx;
    display: flex;
    padding: 20rpx 0;
    justify-content: center;
    align-items: center;
}

.b_desc {
    background-color: #ffffff;
}

.win_icon {
    font-size: 24rpx;
    color: #333333;
}

.none {
    width: 100%;
    height: 116rpx;
}

.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.4);
}

.pop_up {
    width: 100%;
    background-color: #ffffff;
    position: fixed;
    bottom: -700rpx;
    left: 0;
    transition: all 0.5s;
}
.m_left_img{
    width: 180rpx;
    height: 180rpx;
    margin-right: 20rpx;
}
.m_pro_img{
    width: 180rpx;
    height: 180rpx;
}
.m_brand_name{
    margin-top: 30rpx;
    font-size: 28rpx;
    color: #333333;
}
.product_desc {
    width: 100%;
    padding: 20rpx;
    display: flex;
    margin-bottom: 12rpx;
    background-color: #ffffff;
    border-bottom: 1rpx solid #f2f2f2;
}
#show {
    bottom: 0rpx;
}
.time_no{
    font-size: 22rpx;
    color: #888888;
    margin-top: 15rpx;
}
.right_desc {
    width: 100%;
    overflow: hidden;
    padding: 10rpx 0;
}
.icon_view {
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    padding: 20rpx 30rpx;
}

.close_icon {
    width: 26rpx;
    height: 26rpx;
    display: block;
}

.text_tips {
    width: 100%;
    text-align: center;
    font-size: 22rpx;
    color: #333333;
    margin-top: 30rpx;
}

.choose_num {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 20rpx;
}

.choose_num>view {
    width: 100rpx;
    height: 60rpx;
    border: 1rpx solid #dddddd;
    font-size: 36rpx;
    color: #333333;
    display: flex;
    justify-content: center;
    align-items: center;
}

.choose_num .num_value {
    border: 0;
    width: 258rpx;
    height: 60rpx;
    border-top: 1rpx solid #dddddd;
    border-bottom: 1rpx solid #dddddd;
    color: #e72f4b;
    font-size: 36rpx;
    text-align: center;
}

.choose_item {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 30rpx;
}

.choose_item>view {
    width: 92rpx;
    height: 60rpx;
    background-color: #f2f2f2;
    font-size: 22rpx;
    color: #333333;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 30rpx;
}

.choose_item .no_margin {
    margin-right: 0;
}

.total_score {
    margin-top: 30rpx;
    font-size: 28rpx;
    color: #e72f4b;
    text-align: center;
}

.placeholder {
    width: 100%;
    height: 120rpx;
}

.confirm_btn {
    width: 710rpx;
    height: 80rpx;
    background-color: #e72f4b;
    font-size: 28rpx;
    color: #ffffff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 8rpx;
    position: absolute;
    bottom: 20rpx;
    left: 20rpx;
}