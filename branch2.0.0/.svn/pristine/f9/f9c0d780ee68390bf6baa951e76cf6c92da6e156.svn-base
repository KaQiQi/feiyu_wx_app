.swiper{
    width: 750rpx;
    height: 750rpx;
    border-top: 1rpx solid #f2f2f2;
    border-bottom: 1rpx solid #f2f2f2;
}
.slide_img{
    width: 750rpx;
    height: 750rpx;
}
.brand_name{
    font-size: 28rpx;
    color: #333333;
    padding: 20rpx;
}
.user_detail{
    width: 100%;
    border-top: 12rpx solid #f2f2f2;
    padding: 20rpx;
    background-color: #fff1f4;
    display: flex;
    position: relative;
}
.avatar{
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}
.avatar_img{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    display: block;
}
.right_desc{
    width: 100%;
    overflow: hidden;
}
.right_desc>view{
    margin-bottom: 10rpx;
    color: #333333;
    font-size: 24rpx;
}
.win_icon{
    width: 102rpx;
    height: 102rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
}
.top_title {
    position: relative;
    display: flex;
    align-items: center;
    width: 100%;
    padding: 20rpx;
    font-size: 28rpx;
    color: #666666;
    border-top: 12rpx solid #f2f2f2;
}

.left_border {
    width: 4rpx;
    height: 28rpx;
    background-color: #e72f4b;
    margin-right: 10rpx;
}
.draw_btn{
    background-color: #ffffff;
    width: 100%;
    padding: 20rpx;
    position: fixed;
    bottom: 0;
    left: 0;
}
.draw_btn>view{
    width: 100%;
    border-radius: 8rpx;
    background-color: #e72f4b;
    color: #ffffff;
    font-size: 26rpx;
    display: flex;
    padding: 20rpx 0;
    justify-content: center;
    align-items: center;
}
.b_desc{
    background-color: #ffffff;
}
.win_icon{
    font-size: 24rpx;
    color: #333333;
}
.none{
    width: 100%;
    height: 116rpx;
}