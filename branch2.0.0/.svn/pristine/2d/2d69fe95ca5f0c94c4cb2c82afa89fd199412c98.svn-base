.top_banner{
    width: 750rpx;
    height: 500rpx;
    position: relative;
}
.main_img{
    width: 100%;
    height: 100%;
    display: block;
}
.describe{
    width: 100%;
    position: absolute;
    top: 154rpx;
    left: 0;
}
.title{
    width: 100%;
    text-align: center;
    font-size: 28rpx;
    color: #ffffff;
}
.sale_point{
    width: 100%;
    margin-top: 10rpx;
